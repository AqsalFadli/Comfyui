    import subprocess
    import threading
    import time
    import socket

    def launch_tunnel(port):
        while True:
            time.sleep(2)
            sock = socket.socket(socket.AF_INET, socket.SOCK_STREAM)
            if sock.connect_ex(('127.0.0.1', port)) == 0:
                break
            sock.close()
        
        print("\n" + "="*50)
        print("ðŸš€ JONAS AI SYSTEM HAMPIR SIAP...")
